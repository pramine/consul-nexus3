{
  "repositories": {
      "maven2": {
          "proxied": {
              "repo-jenkins-ci-org": {
                  "name": "Proxied repo.jenkins-ci.org",
                  "online": "true",
                  "remote": {
                      "url": "https://repo.jenkins-ci.org/public/",
                      "auth_type": "none"
                  },
                  "blobstore": {
                      "name": "proxied-maven2-repo-jenkins-ci-org",
                      "strict_content_type_validation": "true"
                  },
                  "version_policy": "release",
                  "layout_policy": "strict"
              },
              "maven-central": {
                  "name": "Proxied maven central",
                  "online": "true",
                  "remote": {
                      "url": "https://repo1.maven.org/maven2/",
                      "auth_type": "none"
                  },
                  "blobstore": {
                      "name": "proxied-maven2-maven-central",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "release",
                  "layout_policy": "permissive"
              }
          },
          "hosted": {
              "releases": {
                  "name": "Hosted maven releases",
                  "online": "true",
                  "blobstore": {
                      "name": "hosted-maven2-releases",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "release",
                  "layout_policy": "strict",
                  "write_policy": "allow_write_once"
              },
              "snapshots": {
                  "name": "Hosted maven snapshots",
                  "online": "true",
                  "blobstore": {
                      "name": "hosted-maven2-snapshots",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "snapshot",
                  "layout_policy": "strict",
                  "write_policy": "allow_write"
              }
          }
      }
  },
  "repository-groups": {
      "maven-public": {
          "provider_type": "maven2",
          "online": "true",
          "repositories": [
              "proxied-maven2-maven-central",
              "proxied-maven2-repo-jenkins-ci-org",
              "hosted-maven2-releases",
              "hosted-maven2-snapshots"
          ],
          "blobstore": {
              "name": "group-maven-public",
              "strict_content_type_validation": "true"
          }
      }
  },
  "blobstore": {
      "file": [
          "default",
          "hosted-maven2-snapshots",
          "proxied-maven2-repo-jenkins-ci-org",
          "proxied-maven2-maven-central",
          "hosted-maven2-releases",
          "hosted-maven2-snapshots",
          "group-maven-public"
      ]
  }
}
