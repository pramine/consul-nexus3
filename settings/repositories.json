{
  "repositories": {
      "maven2": {
          "proxy": {
              "repo-jenkins-ci-org": {
                  "name": "Proxied repo.jenkins-ci.org",
                  "online": "true",
                  "remote": {
                      "url": "https://repo.jenkins-ci.org/public/",
                      "auth_type": "none"
                  },
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "true"
                  },
                  "version_policy": "release",
                  "layout_policy": "permissive"
              },
              "maven-central": {
                  "name": "Proxied maven central",
                  "online": "true",
                  "remote": {
                      "url": "https://repo1.maven.org/maven2/",
                      "auth_type": "none"
                  },
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "release",
                  "layout_policy": "permissive"
              }
          },
          "hosted": {
              "releases": {
                  "name": "Hosted maven releases",
                  "online": "true",
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "release",
                  "layout_policy": "strict",
                  "write_policy": "allow_once"
              },
              "snapshots": {
                  "name": "Hosted maven snapshots",
                  "online": "true",
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "false"
                  },
                  "version_policy": "snapshot",
                  "layout_policy": "strict",
                  "write_policy": "allow"
              }
          },
          "group": {
              "maven-public": {
                  "provider_type": "maven2",
                  "online": "true",
                  "repositories": [
                      "proxied-maven2-maven-central",
                      "proxied-maven2-repo-jenkins-ci-org",
                      "hosted-maven2-releases",
                      "hosted-maven2-snapshots"
                  ],
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "true"
                  }
              }
          }
      }
  },
  "blobstores": {
      "file": [
          "maven-default"
      ]
  }
}
