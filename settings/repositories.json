{
  "repositories": {
      "maven2": {
          "proxy": {
              "repo-jenkins-ci-org": {
                  "name": "Proxied repo.jenkins-ci.org",
                  "remote": {
                      "url": "https://repo.jenkins-ci.org/public/"
                  },
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "true"
                  }
              },
              "maven-central": {
                  "name": "Proxied maven central",
                  "remote": {
                      "url": "https://repo1.maven.org/maven2/"
                  },
                  "blobstore": {
                      "name": "maven-default"
                  }
              }
          },
          "hosted": {
              "maven-releases": {
                  "name": "Hosted maven releases",
                  "blobstore": {
                      "name": "maven-default"
                  },
                  "layout_policy": "strict"
              },
              "maven-snapshots": {
                  "name": "Hosted maven snapshots",
                  "blobstore": {
                      "name": "maven-default"
                  },
                  "version_policy": "snapshot",
                  "layout_policy": "strict",
                  "write_policy": "allow"
              }
          },
          "group": {
              "maven-public": {
                  "repositories": [
                      "maven-central",
                      "repo-jenkins-ci-org",
                      "maven-releases",
                      "maven-snapshots"
                  ],
                  "blobstore": {
                      "name": "maven-default",
                      "strict_content_type_validation": "true"
                  }
              }
          }
      }
  },
  "blobstores": {
      "file": [
          "maven-default"
      ]
  }
}
